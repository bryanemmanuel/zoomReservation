<script>
	$(document).ready(function () {
		$("#search").click(function () {
			var revId = $("#revId").val();
			$.ajax({
				url: '@Url.Action("getDetails", "Home")',
				type: 'GET',
				dataType: 'json',
				data: { revId: revId },
				success: function (details) {
					if (details == null) {
						$("#cancelForm").hide();
						var modalBody = $('#appointmentModal').find('.modal-body');
						modalBody.html(
							'<strong class="text-danger">Not Found</strong> '


						);

						$('#appointmentModal').modal('show');
					}
					else {
						var val = details.time;
						var countt = details.count;
						var countt2 = 0;

						var getTimes = [];

						for (var i = 256; i >= 1; i = i / 2) {
							if (i < val && countt2 < countt) {
								switch (i) {
									case 1:
										getTimes.push("8:00AM-9:00AM");
										break;
									case 2:
										getTimes.push("9:00AM-10:00AM");
										break;
									case 4:
										getTimes.push("10:00AM-11:00AM");
										break;
									case 8:
										getTimes.push("11:00AM-12:00PM");
										break;
									case 16:
										getTimes.push("12:00PM-1:00PM");
										break;
									case 32:
										getTimes.push("1:00PM-2:00PM");
										break;
									case 64:
										getTimes.push("2:00PM-3:00PM");
										break;
									case 128:
										getTimes.push("3:00PM-4:00PM");
										break;
									case 256:
										getTimes.push("4:00PM-5:00PM");
										break;
									default:
										break;
								}
								countt2++;
							}

						}

						console.log(val);
						console.log(countt);
						console.log(countt2);
						console.log(getTimes);
						$("#cancelForm").show();
						$("#id").val(details.id);
						var modalBody = $('#appointmentModal').find('.modal-body');
						modalBody.html(


							'<div class="row mb-3" > ' +
							   '<div class=" col-lg-4 col-sm-12 ">' +

							'<label class="col-lg-4 col-sm-12 col-form-label" for="First Name">First Name</label>' +
										'<div class="col-12">' +
											'<input type="text"  class="form-control" value= ' + details.firstName + ' ' + ' disabled />' +
										'</div>' +
								'</div>' +

							'<div class="col-lg-4 col-sm-12">' +
									'<label class="col-sm-3 col-form-label" for="LastName">Last Name</label>' +
										'<div class="col-12">' +
											'<input type="text"  class="form-control" value= ' + details.lastName + ' ' + ' disabled />' +
										'</div>' +
							    '</div>' +


						



						



							'<div class="col-lg-4 col-sm-12">' +
							'<label class="col-sm-5 col-form-label" for="Transaction ID:">Transaction ID:</label>' +
							'<div class="col-12">' +
							'<input type="text"  class="form-control" value= ' + details.transactionId + ' ' + ' disabled />' +
							'</div>' +
							'</div>' +


							'<div class="col-lg-4 col-sm-12">' +
							'<label class="col-sm-5 col-form-label" for="Date ID:">Date</label>' +
							'<div class="col-12">' +
							'<input type="text"  class="form-control" value= ' + details.date + ' ' + ' disabled />' +
							'</div>' +
							'</div>' +



							'<div class="col-lg-4 col-sm-12">' +
							'<label class="col-sm-5 col-form-label" for="Date ID:">Date</label>' +
							'<div class="col-12">' +
							'<input type="text"  class="form-control" value= ' + details.date + ' ' + ' disabled />' +
							'</div>' +
							'</div>' +






							'</div>' +





							
							


					
							'<p><strong>Department:</strong> ' + details.departmentName + ' (' + details.departmentAbbreviation + ')</p>' +
							
							
							
						
							
							
							'<p><strong>Time:</strong> </p>'

						);
						'<div class="row d-flex justify-content-center align-items-center">'
						for (var a = getTimes.length - 1; a >= 0; a--) {
							modalBody.append('<div class="col-3 badge bg-label-success mt-2 mb-2  mx-1" > ' + getTimes[a] + '</div>');
						}
						'<div>'



						$('#appointmentModal').modal('show');
					}

				},
				error: function () {
					alert("Not Found");
				}
			});
		});
	});




